;AACA;;AAbA,GAbA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,SAAA,GAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,WAAA;AAAA,QAAA,KAAA,EAAA,KAAA;AAAA,OAAA;AACA,WAAA,SAAA,GAAA,CAAA,KAAA,SAAA;AACA,WAAA,aAAA,GAAA,CAAA,KAAA,aAAA;AACA,MAAA,aAAA,CAAA,IAAA,CAAA,mBAAA,EAAA,KAAA,KAAA;AACA,MAAA,UAAA,CAAA,MAAA;AACA,aAAA,aAAA,GAAA,CAAA,KAAA,aAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAIA,WAAA,YAAA;AACA,KAXA;;AAaA;AACA,IAAA,YAAA,GAAA;AACA,YAAA,YAAA,GAAA;AACA,QAAA,MAAA,EAAA,KAAA,KAAA,CAAA,IAAA,CAAA,GADA;AAEA,QAAA,MAAA,EAAA,KAAA,KAAA,CAAA;AAFA,OAAA;AAIA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,WAAA;AAAA,QAAA;AAAA,OAAA;AACA,KApBA;;AAqBA;AAEA,UAAA,cAAA,GAAA;AACA,UAAA;AACA,cAAA;AAAA,UAAA;AAAA,YAAA,KAAA,MAAA,CAAA,MAAA;AACA,cAAA,IAAA,GAAA,MAAA,cAAA,CAAA,OAAA,CAAA,MAAA,CAAA;AACA,aAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAAA;AACA,cAAA,MAAA,GAAA,KAAA,IAAA,CAAA,IAAA,CAAA,GAAA;AACA,cAAA,IAAA,GAAA,MAAA,cAAA,CAAA,OAAA,CAAA,MAAA,CAAA;AACA,aAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAAA;AACA;AACA,OARA,CAQA,OAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,wBAAA,EAAA,GAAA;AACA;AACA,KAnCA;;AAqCA,IAAA,UAAA,GAAA;AACA,WAAA,SAAA,GAAA,CAAA,KAAA,SAAA;AACA,KAvCA;;AAyCA,IAAA,cAAA,CAAA,KAAA,EAAA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,WAAA,SAAA,GAAA,CAAA,KAAA,SAAA;AACA;;AA5CA,GA7BA;;AA4EA,EAAA,OAAA,GAAA;AACA,IAAA,UAAA,CAAA,YAAA;AACA,YAAA,KAAA,cAAA,EAAA;AACA,MAAA,aAAA,CAAA,IAAA,CAAA,cAAA,EAAA,KAAA,IAAA,CAAA,GAAA;AACA,KAHA,EAGA,IAHA,CAAA;AAIA,GAjFA;;AAmFA,EAAA,UAAA,EAAA;AACA,IAAA,cADA;AAEA,IAAA;AAFA;AAnFA,CAAA","sourcesContent":["<template>\r\n  <section class=\"details-wrapper main-layout\">\r\n    <section class=\"flex flex-column\" v-if=\"meal && host\">\r\n      <div class=\"title bold\">{{ meal.title }}</div>\r\n      <div>\r\n        <img class=\"star\" src=\"@/assets/icons/star.svg\" />\r\n        <span class=\"rating-details\">\r\n          {{ host.host.rate }}/5 <span>(68 reviews)</span></span\r\n        >\r\n      </div>\r\n      <div class=\"img-gallery\">\r\n        <img\r\n          :src=\"require('@/assets/img/details/chef-details.jpg')\"\r\n          class=\"large-img\"\r\n        />\r\n        <img\r\n          :src=\"require('@/assets/img/details/thai3.jpg')\"\r\n          class=\"medium-img\"\r\n        />\r\n        <img\r\n          :src=\"require('@/assets/img/details/thai2.jpg')\"\r\n          class=\"small-img\"\r\n        />\r\n        <img\r\n          :src=\"require('@/assets/img/details/thai1.jpg')\"\r\n          class=\"small-img\"\r\n        />\r\n      </div>\r\n      <div>\r\n        <img class=\"avatar\" :src=\"require('@/assets/img/img1.jpg')\" />\r\n      </div>\r\n      <div>\r\n        Hosted by <span class=\"host-name bold\">{{ host.fullname }}</span>\r\n      </div>\r\n      <div class=\"main-container\">\r\n        <details-content :meal=\"meal\" :host=\"host\"></details-content>\r\n        <details-booking\r\n          :meal=\"meal\"\r\n          :host=\"host\"\r\n          :user=\"user\"\r\n          @approveBooking=\"approveBooking\"\r\n        ></details-booking>\r\n      </div>\r\n      <div @click=\"closeModal\" v-if=\"isBooking\" class=\"dark-screen\">\r\n        <div class=\"booking-modal flex flex-column\" v-if=\"isBooking\">\r\n          <div class=\"booking-content\">\r\n            <div class=\"img\">\r\n              <img :src=\"require(`@/assets/img/previews/${meal.imgUrl}.jpg`)\" />\r\n            </div>\r\n            <div class=\"content\">\r\n              <h4>You're almost there!</h4>\r\n              <h3 class=\"bold\">{{ meal.title }}</h3>\r\n              <div><span class=\"bold\"></span>{{ order.eventTime }}</div>\r\n              <div>${{ meal.price }} per person</div>\r\n              <div>{{ order.guestsNum }} guests</div>\r\n              <div><span class=\"bold\">Total: </span>${{ totalPrice }}</div>\r\n            </div>\r\n          </div>\r\n          <div class=\"btns\">\r\n            <button class=\"btn-book\" @click=\"saveOrder\">Approve</button>\r\n            <button class=\"btn-cancel\" @click=\"closeModal\">Cancel</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"booking-modal booked\" v-if=\"isOrderPlaced\">\r\n        <h3>Your order has been placed!</h3>\r\n      </div>\r\n    </section>\r\n\r\n    <div v-else v-loading=\"loading\"></div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport { NEWmealService } from \"../services/NEW-meal-service.js\";\r\nimport { NEWuserService } from \"../services/NEW-user-service.js\";\r\nimport detailsContent from \"../cmps/details-content.vue\";\r\nimport detailsBooking from \"../cmps/details-booking.vue\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      meal: null,\r\n      host: null,\r\n      user: this.$store.getters.loggedinUser,\r\n      isBooking: false,\r\n      isOrderPlaced: false,\r\n      order: {},\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    userImg() {\r\n      return this.user.imgUrl;\r\n    },\r\n\r\n    mealImg() {\r\n      return this.meal.mealImgUrl;\r\n    },\r\n\r\n    totalPrice() {\r\n      const totalPrice = this.order.guestsNum * this.meal.price;\r\n      this.order.totalPrice = totalPrice;\r\n      return totalPrice;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    saveOrder() {\r\n      this.$store.dispatch({ type: \"saveOrder\", order: this.order });\r\n      this.isBooking = !this.isBooking;\r\n      this.isOrderPlaced = !this.isOrderPlaced;\r\n      socketService.emit(\"details-add-order\", this.order);\r\n      setTimeout(() => {\r\n        this.isOrderPlaced = !this.isOrderPlaced;\r\n      }, 3000);\r\n\r\n      this.updateGuests();\r\n    },\r\n\r\n    // --------------------------------\r\n    updateGuests() {\r\n      const updateGuests = {\r\n        mealId: this.order.meal._id,\r\n        guests: this.order.guestsNum,\r\n      };\r\n      this.$store.dispatch({ type: \"addGuests\", updateGuests });\r\n    },\r\n    // --------------------------------\r\n\r\n    async getMealAndUser() {\r\n      try {\r\n        const { mealId } = this.$route.params;\r\n        const meal = await NEWmealService.getById(mealId);\r\n        this.meal = JSON.parse(JSON.stringify(meal));\r\n        const userId = this.meal.host._id;\r\n        const user = await NEWuserService.getById(userId);\r\n        this.host = JSON.parse(JSON.stringify(user));\r\n        return;\r\n      } catch (err) {\r\n        console.log(\"err in getMealAndUser:\", err);\r\n      }\r\n    },\r\n\r\n    closeModal() {\r\n      this.isBooking = !this.isBooking;\r\n    },\r\n\r\n    approveBooking(order) {\r\n      this.order = order;\r\n      this.isBooking = !this.isBooking;\r\n    },\r\n  },\r\n\r\n  created() {\r\n    setTimeout(async () => {\r\n      await this.getMealAndUser();\r\n      socketService.emit(\"details-host\", this.host._id);\r\n    }, 1000);\r\n  },\r\n\r\n  components: {\r\n    detailsContent,\r\n    detailsBooking,\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/pages"}]}