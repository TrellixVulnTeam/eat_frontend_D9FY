EAAA;AACA,YAAA,KAAA,GAAA;AACA,QAAA,GAAA,EAAA,OADA;AAEA,QAAA,QAAA,EAAA,cAFA;AAGA,QAAA,MAAA,EAAA;AAHA,OAAA;AAKA,WAAA,KAAA,CAAA,KAAA,GAAA,KAAA;AACA,YAAA,IAAA,GAAA;AACA,QAAA,GAAA,EAAA,QAAA,CAAA,GADA;AAEA,QAAA,KAAA,EAAA,QAAA,CAAA;AAFA,OAAA;AAIA,WAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,YAAA,IAAA,GAAA;AACA,QAAA,GAAA,EAAA,QAAA,CAAA,GADA;AAEA,QAAA,QAAA,EAAA,QAAA,CAAA,QAFA;AAGA,QAAA,MAAA,EAAA,QAAA,CAAA;AAHA,OAAA;AAKA,WAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,KAvBA;;AAyBA,IAAA,SAAA,GAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,WAAA;AAAA,QAAA,KAAA,EAAA,KAAA;AAAA,OAAA;AACA,WAAA,SAAA,GAAA,CAAA,KAAA,SAAA;AACA,WAAA,aAAA,GAAA,CAAA,KAAA,aAAA;AACA,MAAA,UAAA,CAAA,MAAA;AACA,aAAA,aAAA,GAAA,CAAA,KAAA,aAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA,KAhCA;;AAiCA,UAAA,cAAA,GAAA;AACA,UAAA;AACA,cAAA;AAAA,UAAA;AAAA,YAAA,KAAA,MAAA,CAAA,MAAA;AACA,cAAA,IAAA,GAAA,MAAA,WAAA,CAAA,OAAA,CAAA,MAAA,CAAA;AACA,aAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAAA;AACA,cAAA,MAAA,GAAA,KAAA,IAAA,CAAA,IAAA,CAAA,GAAA;AACA,cAAA,IAAA,GAAA,MAAA,WAAA,CAAA,OAAA,CAAA,MAAA,CAAA;AACA,aAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAAA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,IAAA;AACA,OARA,CAQA,OAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,wBAAA,EAAA,GAAA;AACA;AACA;;AA7CA,GA5BA;;AA4EA,EAAA,OAAA,GAAA;AACA,SAAA,cAAA;AACA;;AA9EA,CAAA","sourcesContent":["<template>\r\n<section class=\"details-wrapper main-layout\">\r\n\r\n  <section class=\"flex flex-column\" v-if=\"meal && user\">\r\n    <div class=\"img-gallery\">\r\n      <img :src=\"require('@/assets/img/chef-details.jpg')\" class=\"large-img\" />\r\n      <img :src=\"require('@/assets/img/meal.jpg')\" class=\"medium-img\" />\r\n      <img :src=\"require('@/assets/img/pizza.jpg')\" class=\"small-img\" />\r\n      <img :src=\"require('@/assets/img/plates.jpg')\" class=\"small-img\" />\r\n    </div>\r\n    <div class=\"main-container flex\">\r\n      <div class=\"details-container flex flex-column\">\r\n        <div class=\"title bold\">{{ meal.title }}</div>\r\n         <div >\r\n                Hosted by <span class=\"host-name bold\" >{{ user.fullname }}</span>\r\n              </div>\r\n        <div class=\"summary\">\r\n          <!-- <h3>Experience in a nutshell</h3> -->\r\n          <div class=\"summary-container\"></div>\r\n        </div>\r\n        <div class=\"meal-desc\">\r\n          <p class=\" bold\">A word about the experience</p>\r\n          <!-- <p>{{ meal.description }}</p> -->\r\n          <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. </p>\r\n            <p> Saepe provident in ducimus odio obcaecati.  </p>\r\n            <p> Ducimus, itaque dolore debitis incidunt animi quo,  </p>\r\n           <p>  dolor dolorum ut quaerat est officiis ea architecto unde?</p>\r\n        </div>\r\n        <!-- <div class=\"host-container light-border\">\r\n       \r\n          <div class=\"host-desc\">{{ user.host.desc }}</div>\r\n        </div> -->\r\n        <p class=\"meal-menu bold\">Menu</p>\r\n          <p>Starters</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>Main</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>Desert</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n          <p>-</p>\r\n        <div class=\"reviews-container\">\r\n          <p class=\"bold\">Some of our guests reviews</p>\r\n          <ul class=\"reviews-list\">\r\n            <li\r\n              v-for=\"review in user.host.reviews\"\r\n              :key=\"review._id\"\r\n              class=\"review-item\"\r\n            >\r\n              <p class=\"bold\">Guest Guestovsky</p>\r\n              <p>{{ review.txt }}</p>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n      <div class=\"booking-container light-border flex flex-column\">\r\n        <div>\r\n        <span class=\"bold price-booking\">${{ meal.price }}</span> \r\n        <span> Price per guest </span> \r\n        <div>\r\n            <img class=\"host-avatar\" :src=\"require('@/assets/img/img1.jpg')\" />\r\n            <div>\r\n              <div>Rating <span class=\"rating\">{{ user.host.rate }}</span></div>\r\n            </div>\r\n        </div>\r\n        </div>\r\n        <div>\r\n        <p class=\"bold\">Date</p>\r\n        <input type=\"date\" v-model=\"order.eventTime\" />\r\n        </div>\r\n        <div>\r\n        <p class=\"bold\">Number of guests</p>\r\n        <div><input type=\"number\" v-model=\"order.guestsNum\" /></div>\r\n        </div>\r\n        <div><button class=\"btn-book\" @click=\"toggleIsBooking\">Book my seats</button></div>\r\n      </div>\r\n    </div>\r\n    <div @click=\"toggleIsBooking\" v-if=\"isBooking\" class=\"dark-screen\">\r\n      <div class=\"booking-modal\" v-if=\"isBooking\">\r\n        <h4>You're almost there!</h4>\r\n        <h3 class=\"bold\">{{ meal.title }}</h3>\r\n        <div><span class=\"bold\">Price per person: </span>{{ meal.price }}</div>\r\n        <div><span class=\"bold\">Num of guests: </span>{{ order.guestsNum }}</div>\r\n        <div><span class=\"bold\">Meal time: </span>{{ order.eventTime }}</div>\r\n        <div><span class=\"bold\">Total price: </span>{{ totalPrice }}</div>\r\n        <button class=\"btn-cancel\" @click=\"toggleIsBooking\">Cancel</button>\r\n        <button class=\"btn-book\" @click=\"saveOrder\">Book me in!</button>\r\n      </div>\r\n    </div>\r\n    <div class=\"booking-modal booked\" v-if=\"isOrderPlaced\">\r\n      <h3>Your order has been placed!</h3>\r\n    </div>\r\n  </section>\r\n</section>\r\n</template>\r\n\r\n<script>\r\nimport { mealService } from \"../services/meal-service.js\";\r\nimport { userService } from \"../services/user-service.js\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      meal: null,\r\n      user: null,\r\n      isBooking: false,\r\n      isOrderPlaced: false,\r\n      order: {\r\n        _id:null,\r\n        guestsNum: 0,\r\n        eventTime: null,\r\n        createdAt: Date.now(),\r\n        totalPrice: null,\r\n      },\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    userImg() {\r\n      return this.user.imgUrl;\r\n    },\r\n    totalPrice() {\r\n      const totalPrice = this.order.guestsNum * this.meal.price;\r\n      this.order.totalPrice = totalPrice;\r\n      return totalPrice;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    toggleIsBooking() {\r\n      this.isBooking = !this.isBooking;\r\n    },\r\n\r\n    createOrder(currMeal, currUser) {\r\n      const buyer = {\r\n        _id: \"u1001\",\r\n        fullname: \"user userson\",\r\n        imgUrl: \"\",\r\n      };\r\n      this.order.buyer = buyer;\r\n      const meal = {\r\n        _id: currMeal._id,\r\n        title: currMeal.title,\r\n      };\r\n      this.order.meal = meal;\r\n      const host = {\r\n        _id: currUser._id,\r\n        fullname: currUser.fullname,\r\n        imgUrl: currUser.imgUrl,\r\n      };\r\n      this.order.host = host;\r\n    },\r\n\r\n    saveOrder() {\r\n      this.$store.dispatch({ type: \"saveOrder\", order: this.order });\r\n      this.isBooking = !this.isBooking;\r\n      this.isOrderPlaced = !this.isOrderPlaced;\r\n      setTimeout(()=>{\r\n        this.isOrderPlaced = !this.isOrderPlaced\r\n      },3000)\r\n    },\r\n    async getMealAndUser() {\r\n      try {\r\n        const { mealId } = this.$route.params;\r\n        const meal = await mealService.getById(mealId);\r\n        this.meal = JSON.parse(JSON.stringify(meal));\r\n        const userId = this.meal.host._id;\r\n        const user = await userService.getById(userId);\r\n        this.user = JSON.parse(JSON.stringify(user));\r\n        this.createOrder(meal, user);\r\n      } catch (err) {\r\n        console.log(\"err in getMealAndUser:\", err);\r\n      }\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.getMealAndUser();\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/pages"}]}