t.forEach((meal) => {\r\n        // meal.shortTitle = meal.title.substring(0, 8);\r\n\r\n        mealTitle.push(meal.title);\r\n      });\r\n      return mealTitle;\r\n    },\r\n  },\r\n\r\n  // -----------------------------------\r\n\r\n  created() {\r\n    // this.$store.dispatch({ type: \"loadMeals\" });\r\n     this.meals.forEach((meal) => {\r\n        return meal.shortTitle = meal.title.substring(0,9)\r\n      });\r\n  },\r\n  // -----------------------------------\r\n  components: {\r\n    chart,\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/cmps"}]}