=\"radio\" value=\"Love\" v-model=\"topic\" @change=\"changeTopic\" /> \n      Love\n    </label> -->\n    <ul>\n      <li v-for=\"(msg, idx) in msgs\" :key=\"idx\">\n        <span>{{msg.from}}:</span>{{msg.txt}}\n      </li>\n    </ul>\n    <hr />\n    <form @submit.prevent=\"sendMsg\">\n      <input type=\"text\" v-model=\"msg.txt\" placeholder=\"Your msg\"/>\n      <button>Send</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport {socketService} from '@/services/socket.service';\n\nexport default {\n  data() {\n    return {\n      msg: {from: null, txt: ''},\n      msgs: [],\n      topic : 'Love'\n    }\n  },\n\n  computed: {\n    userName (){\n      this.msg.from = this.$store.getters.loggedinUser.fullname\n    }\n  },\n  created() {\n    // socketService.setup();\n    socketService.emit('chat topic', this.topic)\n    socketService.on('chat addMsg', this.addMsg)\n    console.log(this.userName)\n  },\n  destroyed() {\n    socketService.off('chat addMsg', this.addMsg)\n    // socketService.terminate();\n  },\n  methods: {\n    addMsg(msg) {\n      this.msgs.push(msg)\n      console.log(this.msgs)\n    },\n    sendMsg() {\n      console.log('Sending', this.msg);\n      socketService.emit('chat newMsg', this.msg)\n      // TODO: next line not needed after connecting to backend\n      // this.addMsg(this.msg)\n      // setTimeout(()=>this.addMsg({from: 'Dummy', txt: 'Yey'}), 2000)\n      this.msg = {from: 'Me', txt: ''};\n    },\n    changeTopic() {\n      socketService.emit('chat topic', this.topic)\n    }\n  }\n}\n</script>\n"]}]}